{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./server/src/bin/www",
    "server": "babel-node ./server/src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/src/bin/www",
    "clean": "rimraf dist",
    "build": "babel ./server/src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.9",
    "morgan": "~1.9.1",
    "node-bitlyapi": "^0.4.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "faker": "^4.1.0"
  }
}
